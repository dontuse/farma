{{>head}}

<body class="b-body">

<div class="b-page">
    <div class="b-page__wrap">

        {{>header}}

        <div class="b-wrapper">
            <div class="b-content">
                <section class="b-grid">

                    <aside class="b-grid__col b-grid__col_2">
                        <div class="b-text">
                            <h1>Статика</h1>
                            <div>
                                <ul>
                                    {{#each  pages}}
                                        <li>{{data.name}}
                                            <a href="{{pageName}}"> {{basename }} </a>
                                        </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </aside>

                </section>

            </div>
        </div>


    </div>
    <footer class="b-footer"></footer>
</div>


<script src="{{srcJs}}"></script>

</body>
</html>

